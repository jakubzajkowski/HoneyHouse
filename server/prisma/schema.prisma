// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Products {
  id String @id @default(uuid())
  category String
  name  String
  img String
  desc String
  weight Int
  price Float
  quantity Int
  alergic_info String
  usage String
  taste String
  expire_date String 
}
